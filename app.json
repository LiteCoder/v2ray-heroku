     },
    "PORT": {
       "description": "监听的端口号，可不用修改",
       "value": "10000"
     }
  },
  "website": "https://github.com/LiteCoder/v2ray-heroku",
  "repository": "https://github.com/LiteCoder/v2ray-heroku",
  "stack": "container"
}
