{
  "name": "V2ray",
  "description": "deploy V2ray to Heroku.",
  "keywords": ["V2ray"],
  "env": {
    "VER": {
       "description": "输入v2ray Core 版本号（latest表示最新版本）",
       "value": "latest"
     }, 
    "UUID": {
       "description": "Vmess协议默认UUID，请输入自己的UUID",
